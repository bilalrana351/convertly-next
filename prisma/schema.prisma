datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
  directUrl = env("MONGO_URI")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  username String @unique
  // Add other fields as needed
}